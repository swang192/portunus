{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "NODE_ENV=production next start",
    "pretty-lint": "pretty-quick --check --branch",
    "eslint": "node_modules/.bin/eslint --ext .js,.jsx .",
    "lint": "eslint --ext .js,.jsx .",
    "fix": "yarn lint --fix",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "cypress": "BABEL_ENV=development cypress run",
    "cypress-open": "BABEL_ENV=development cypress open",
    "collect-reports": "mkdir reports && cp cypress-coverage/coverage-final.json reports/cypress.json && cp jest-coverage/coverage-final.json reports/jest.json",
    "merge-reports": "nyc merge reports && mv coverage.json .nyc_output/out.json",
    "full-coverage-report": "nyc report --reporter=lcov --reporter=text-summary"
  },
  "nyc": {
    "report-dir": "cypress-coverage"
  },
  "dependencies": {
    "@bequestinc/wui": "^1.4.2",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@sentry/react": "^5.20.1",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "js-cookie": "^2.2.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "next": "^9.5.3",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.6.2",
    "next-svgr": "^0.0.2",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-axe": "^3.5.4",
    "react-dom": "16.13.0",
    "react-ga": "^3.3.0",
    "react-password-strength-bar": "^0.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@cypress/code-coverage": "^3.8.1",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/dom": "^7.28.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.1.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "be-pretty": "^0.9.5",
    "css-mediaquery": "^0.1.2",
    "cypress": "^6.3.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
